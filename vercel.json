{
  "buildCommand": "cd backend && pip install -r requirements.txt",
  "installCommand": "pip install -r requirements.txt",
  "outputDirectory": ".",
  "functions": {
    "backend/app/main.py": {
      "runtime": "python3.12",
      "memory": 1024,
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "backend/app/main.py"
    }
  ]
}
